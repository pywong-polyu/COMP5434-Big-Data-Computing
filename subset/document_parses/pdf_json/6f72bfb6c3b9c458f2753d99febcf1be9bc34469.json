    "metadata": {
        "title": "Electronic cigarettes and vaping: toxicological awareness is increasing",
        "authors": [
            {
                "first": "Hermann",
                "middle": [
                    "M"
                ],
                "last": "Bolt",
                "suffix": "",
                "affiliation": {
                    "laboratory": "Leibniz Research Centre for Working Environment and Human Factors at TU",
                    "institution": "",
                    "location": {
                        "addrLine": "Ardeystr. 67",
                        "postCode": "44139, 1784",
                        "settlement": "Dortmund (IfADo), Dortmund",
                        "country": "Germany"
