                    "text": "[7,",
                    "ref_id": "BIBREF6"
                },
                {
                    "start": 488,
                    "end": 490,
                    "text": "8]",
                    "ref_id": "BIBREF7"
                },
                {
                    "start": 635,
                    "end": 638,
                    "text": "[5]",
                    "ref_id": "BIBREF4"
                },
                {
                    "start": 827,
                    "end": 830,
                    "text": "[7]",
